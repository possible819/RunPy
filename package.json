{
  "name": "run-py",
  "version": "1.0.0",
  "description": "Runner for Python",
  "main": "dist/index.js",
  "repository": "https://github.com/possible819/RunPy",
  "author": "Jay Lee <jaylee.possible@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"webpack -w --config webpack.config.dev.js\" \"tsc && electron .\"",
    "clean": "rm -r dist",
    "build": "webpack && tsc",
    "build:dev": "webpack --config ./webpack.config.dev.js && tsc",
    "packaging:mac": "electron-packager . --overwrite --asar --platform darwin --arch x64 --out packages",
    "packaging:win": "electron-packager . --overwrite --asar --platform win32 --arich x64 --out packages",
    "packaging:linux": "electron-packager . --overwrite --asar --platform linux --arch x64 --out packages",
    "packaging": "yarn packaging:mac && yarn packaging:win && yarn packaging:linux",
    "packaging:clean": "rm -r ./packages",
    "installer:debian": "electron-installer-debian --src ./packages/run-py-linux-x64/ --dest ./installer/ --arch amd64",
    "installer:clean": "rm -r ./installers"
  },
  "devDependencies": {
    "asar": "^3.0.3",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "electron": "^10.1.2",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^15.1.0",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@types/codemirror": "0.0.98",
    "codemirror": "^5.58.1",
    "lit-element": "^2.4.0"
  }
}